  "_comment": "This file is generated. Please edit .homeycompose/app.json instead.",
  "id": "com.yandex",
  "version": "1.0.0",
  "platforms": [
    "local"
  ],
  "compatibility": ">=12.1.0",
  "sdk": 3,
  "brandColor": "#FC3F1D",
  "name": {
    "en": "Yandex",
    "ru": "Яндекс"
  },
  "description": {
    "en": "There will be everything",
    "ru": "Найдётся всё"
  },
  "category": [
    "music"
  ],
  "permissions": [],
  "images": {
    "small": "/assets/images/small.jpg",
    "large": "/assets/images/large.jpg",
    "xlarge": "/assets/images/xlarge.jpg"
  },
  "support": "mailto:tolls.gospel-0w@icloud.com",
  "author": {
    "name": "Alexey Krainev",
    "email": "tolls.gospel-0w@icloud.com"
  },
  "source": "https://github.com/kraineff/homey-yandex",
  "bugs": {
    "url": "https://github.com/kraineff/homey-yandex/issues"
  },
  "flow": {
    "triggers": [
      {
        "highlight": true,
        "title": {
          "en": "Scenario is started",
          "ru": "Сценарий запущен"
        },
        "titleFormatted": {
          "en": "Scenario is started: [[scenario]]",
          "ru": "Сценарий запущен: [[scenario]]"
        },
        "args": [
          {
            "name": "scenario",
            "type": "autocomplete",
            "title": {
              "en": "Scenario",
              "ru": "Сценарий"
            },
            "placeholder": {
              "en": "Enter a name to create",
              "ru": "Введите название для создания"
            }
          }
        ],
        "id": "scenario_run"
      }
    ],
    "actions": [
      {
        "highlight": true,
        "title": {
          "en": "Run a command",
          "ru": "Выполнить команду"
        },
        "titleFormatted": {
          "en": "Run [[command]] with [[volume]]",
          "ru": "Выполнить [[command]] с [[volume]]"
        },
        "args": [
          {
            "name": "command",
            "type": "text",
            "title": {
              "en": "Command",
              "ru": "Команда"
            },
            "placeholder": {
              "en": "What's the weather like now?",
              "ru": "Какая сейчас погода?"
            }
          },
          {
            "name": "volume",
            "type": "range",
            "title": {
              "en": "Volume",
              "ru": "Громкость"
            },
            "required": false,
            "label": "%",
            "min": 0,
            "max": 1,
            "step": 0.1,
            "labelMultiplier": 100,
            "labelDecimals": 0
          },
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=dexp-smartbox|elari-smartbeat|irbis-a|jbl-link-music|jbl-link-portable|lg-xboom-ai-thinq|prestigio-smart-mate|yandex-station|yandex-station-2|yandex-station-duo-max|yandex-station-lite|yandex-station-max|yandex-station-midi|yandex-station-mini|yandex-station-mini-2"
          }
        ],
        "tokens": [
          {
            "name": "response",
            "type": "string",
            "title": {
              "en": "Response",
              "ru": "Результат"
            }
          }
        ],
        "id": "media_run"
      },
      {
        "highlight": true,
        "title": {
          "en": "Text-to-Speech",
          "ru": "Произнести текст"
        },
        "titleFormatted": {
          "en": "Say [[text]] with [[volume]]",
          "ru": "Произнести [[text]] с [[volume]]"
        },
        "args": [
          {
            "name": "text",
            "type": "text",
            "title": {
              "en": "Text",
              "ru": "Текст"
            },
            "placeholder": {
              "en": "Hello, world!",
              "ru": "Привет, мир!"
            }
          },
          {
            "name": "volume",
            "type": "range",
            "title": {
              "en": "Volume",
              "ru": "Громкость"
            },
            "required": false,
            "label": "%",
            "min": 0,
            "max": 1,
            "step": 0.1,
            "labelMultiplier": 100,
            "labelDecimals": 0
          },
          {
            "type": "device",
            "name": "device",
            "filter": "driver_id=dexp-smartbox|elari-smartbeat|irbis-a|jbl-link-music|jbl-link-portable|lg-xboom-ai-thinq|prestigio-smart-mate|yandex-station|yandex-station-2|yandex-station-duo-max|yandex-station-lite|yandex-station-max|yandex-station-midi|yandex-station-mini|yandex-station-mini-2"
          }
        ],
        "id": "media_say"
      }
    ]
  },
